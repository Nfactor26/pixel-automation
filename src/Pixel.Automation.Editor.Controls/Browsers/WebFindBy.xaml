<UserControl x:Class="Pixel.Automation.Editor.Controls.Browsers.WebFindBy"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Pixel.Automation.Editor.Controls.Browsers"
             mc:Ignorable="d" x:Name="WebFindByControl"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <ComboBox Name="FindByCombo" ItemsSource="{Binding AvailableStrategies, ElementName=WebFindByControl}"
                  SelectedValue="{Binding FindBy, ElementName=WebFindByControl}"/>
    </Grid>
</UserControl>
